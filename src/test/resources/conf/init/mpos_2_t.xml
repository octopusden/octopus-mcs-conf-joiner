
<CHANNEL NAME="MPOS_2_TCP">
  <PARAMETER NAME="MESSTRACE" VALUE="OFF"/>
  <PARAMETER NAME="USERINTERFACE" VALUE="CONSOLE"/>
  <PARAMETER NAME="EXTERNAL_IS0" VALUE="omni_395.prm"/>
  <PARAMETER NAME="OPTIONAL_IS0" VALUE="omni395q.prm"/>
  <PARAMETER NAME="RID_ROUTING" VALUE="ON"/>
  <PARAMETER NAME="ENCRYPTION" VALUE="ON"/>
  <MODULE TYPE="FORMAT">
    <PARAMETER NAME="DLL" VALUE="lib/libengine4m.so"/>
    <PARAMETER NAME="FUNCTION" VALUE="IS08583_FORMAT"/>
    <PARAMETER NAME="DEBUG" VALUE="OFF"/>
    <PARAMETER NAME="MESS_FORMAT" VALUE="POS_IS0"/>
  </MODULE>
  <MODULE TYPE="ANALYSE">
    <PARAMETER NAME="DLL" VALUE="lib/libengine4m.so"/>
    <PARAMETER NAME="FUNCTION" VALUE="IS08583_ANALYSE"/>
    <PARAMETER NAME="DEBUG" VALUE="OFF"/>
  </MODULE>
  <MODULE TYPE="NETWORK">
    <PARAMETER NAME="DLL" VALUE="posctrl/libposctrl.so"/>
    <PARAMETER NAME="FUNCTION" VALUE="POS_NETWORK"/>
    <PARAMETER NAME="CONVERT_TPK_ZPK" VALUE="OFF"/>
    <PARAMETER NAME="COMS_LOG_MODE" VALUE="FULL"/>
    <PARAMETER NAME="ACQUIRER_FEE" VALUE="ON"/>
    <PARAMETER NAME="CURRENCY_643_TO_810" VALUE="ON"/>
    <PARAMETER NAME="OPERATION_MODE" VALUE="CRYPTO_SW_MODE"/>
    <PARAMETER NAME="DERIV_KEY_A" VALUE="0123456789ABCDEF"/>
    <PARAMETER NAME="GUARD_TIME" VALUE="7200" COMMENT="in sec"/>
    <!--PARAMETER NAME="CREDIT_OFFLINE_CARD_VERIFICATION_CHANNELS" VALUE="E,V"/-->
    <PARAMETER NAME="DEBUG" VALUE="OFF"/>
  </MODULE>
  <MODULE TYPE="INTERNAL">
    <PARAMETER NAME="DLL" VALUE="posctrl/libposctrl.so"/>
    <PARAMETER NAME="FUNCTION" VALUE="POS_INTERNAL"/>
    <PARAMETER NAME="DEBUG" VALUE="OFF"/>
    <PARAMETER NAME="ENGINE97_SUPPORT" VALUE="YES"/>
  </MODULE>
  <MODULE TYPE="DBACCESS">
    <PARAMETER NAME="CHANNEL_ID" VALUE="P2"/>
    <!--PARAMETER NAME="SQLINIFILE" VALUE="conf/sql/mptcp1.sql"/-->

    <PARAMETER NAME="TABLE1" VALUE="COMS_LOG"/>
    <PARAMETER NAME="DEBUG" VALUE="OFF"/>
    <PARAMETER NAME="INCLUDE" VALUE="conf/init/connect.xml"/>
    <PARAMETER NAME="MAX_SESSIONS" VALUE="7"/>
  </MODULE>
  <MODULE TYPE="RECEIVETYPE">
    <PARAMETER NAME="DLL" VALUE="lib/libtcplinks.so"/>
    <PARAMETER NAME="FUNCTION" VALUE="TCP_MULTI_XF"/>
    <PARAMETER NAME="PROTOCOL" VALUE="TCP_SERVER"/>
    <PARAMETER NAME="LISTENING_TIMEOUT" VALUE="600" COMMENT="in sec"/>
    <PARAMETER NAME="IP_PORT" VALUE="1212"/>
    <PARAMETER NAME="HEADER_TYPE" VALUE="BIN_LENGTH_2"/>
    <PARAMETER NAME="RCV_TIMEOUT" VALUE="55" COMMENT="in sec"/>
    <PARAMETER NAME="CHECK_RECALLS" VALUE="OFF"/>
    <PARAMETER NAME="DEBUG" VALUE="OFF"/>
  </MODULE>
  <MODULE TYPE="TRANSMITTYPE">
    <PARAMETER NAME="DLL" VALUE="lib/libtcplinks.so"/>
    <PARAMETER NAME="FUNCTION" VALUE="TCP_MULTI_XF"/>
    <PARAMETER NAME="POS_NETWORK" VALUE="ON"/>
    <PARAMETER NAME="MAX_CONNECTIONS" VALUE="32"/>
    <PARAMETER NAME="DEBUG" VALUE="OFF"/>
  </MODULE>
  <MODULE TYPE="ROUTING">
    <PARAMETER NAME="CFILE" VALUE="conf/tables/routing.xml"/>
  </MODULE>
</CHANNEL>
