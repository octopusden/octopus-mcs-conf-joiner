
<CHANNEL NAME="DATAGATE_INF">
  <PARAMETER NAME="MESSTRACE" VALUE="OFF"/>
  <PARAMETER NAME="USERINTERFACE" VALUE="CONSOLE"/>
  <PARAMETER NAME="ENCRYPTION" VALUE="ON"/>
  <MODULE TYPE="NETWORK">
    <PARAMETER NAME="DLL" VALUE="datagate/libdatagate.so"/>
    <PARAMETER NAME="FUNCTION" VALUE="DATAGATE_NETWORK"/>
    <PARAMETER NAME="PARAMETERFILE" VALUE="conf/tables/datagate_info.xml"/>
    <!--PARAMETER NAME="PARAMETERFILE" VALUE="conf/tables/datagate_demo.xml"/-->

    <PARAMETER NAME="SOAPLOGDIR" VALUE="soaplogs"/>
    <PARAMETER NAME="FILE_BASE_DIR" VALUE=""/>
    <PARAMETER NAME="OPERATION_MODE" VALUE="CRYPTO_SW_MODE"/>
    <PARAMETER NAME="DERIV_KEY_A" VALUE="FBA6307146D2A837"/>
    <PARAMETER NAME="KEY_VARIANT" VALUE="1"/>
    <PARAMETER NAME="GUARD_TIME" VALUE="7200" COMMENT="in sec"/>
    <PARAMETER NAME="DEBUG" VALUE="OFF"/>
  </MODULE>
  <MODULE TYPE="DBACCESS">
    <PARAMETER NAME="CHANNEL_ID" VALUE="DG"/>
    <!--PARAMETER NAME="SQLINIFILE" VALUE="conf/sql/datagate.sql"/-->

    <PARAMETER NAME="INCLUDE" VALUE="conf/init/connect.xml"/>
    <PARAMETER NAME="DEBUG" VALUE="OFF"/>
  </MODULE>
  <MODULE TYPE="RECEIVETYPE">
    <PARAMETER NAME="DLL" VALUE="lib/libtcplinks.so"/>
    <PARAMETER NAME="FUNCTION" VALUE="TCP_MULTI_XF"/>
    <PARAMETER NAME="PROTOCOL" VALUE="TCP_SERVER"/>
    <PARAMETER NAME="IP_PORT" VALUE="9989"/>
    <PARAMETER NAME="HEADER_TYPE" VALUE="NONE"/>
    <PARAMETER NAME="SPLITED_RECEIVER" VALUE="ON"/>
    <PARAMETER NAME="KEEP_LISTENER" VALUE="ON"/>
    <PARAMETER NAME="LISTENING_TIMEOUT" VALUE="120" COMMENT="in sec"/>
    <PARAMETER NAME="RCV_TIMEOUT" VALUE="55" COMMENT="in sec"/>
    <PARAMETER NAME="DEBUG" VALUE="OFF"/>
  </MODULE>
  <MODULE TYPE="TRANSMITTYPE">
    <PARAMETER NAME="DLL" VALUE="lib/libtcplinks.so"/>
    <PARAMETER NAME="FUNCTION" VALUE="TCP_MULTI_XF"/>
    <PARAMETER NAME="POS_NETWORK" VALUE="ON"/>
    <PARAMETER NAME="MAX_CONNECTIONS" VALUE="3"/>
    <PARAMETER NAME="DEBUG" VALUE="OFF"/>
  </MODULE>
  <MODULE TYPE="ROUTING">
    <PARAMETER NAME="CFILE" VALUE="conf/tables/routing.xml"/>
  </MODULE>
</CHANNEL>
