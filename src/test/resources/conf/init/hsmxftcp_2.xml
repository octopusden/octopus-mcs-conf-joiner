
<CHANNEL NAME="HSM_XFACE_2">
  <PARAMETER NAME="MESSTRACE" VALUE="OFF"/>
  <PARAMETER NAME="USERINTERFACE" VALUE="CONSOLE"/>
  <MODULE TYPE="FORMAT">
    <PARAMETER NAME="DLL" VALUE="hsmxface/libhsmxface.so"/>
    <PARAMETER NAME="FUNCTION" VALUE="HSM_FORMATER"/>
    <PARAMETER NAME="HSM_DATA_FORMAT" VALUE="BIN"/>
    <PARAMETER NAME="INHERIT_KEY_SCHEME" VALUE="ON"/>
    <!--PARAMETER NAME="GISKE_FORMAT_SUPPORT" VALUE="ON"/-->
    <PARAMETER NAME="DEBUG" VALUE="OFF"/>
  </MODULE>
  <MODULE TYPE="ANALYSE">
    <PARAMETER NAME="DLL" VALUE="hsmxface/libhsmxface.so"/>
    <PARAMETER NAME="FUNCTION" VALUE="HSM_ANALYZER"/>
    <PARAMETER NAME="DEBUG" VALUE="OFF"/>
  </MODULE>
  <MODULE TYPE="INTERNAL">
    <PARAMETER NAME="DLL" VALUE="hsmxface/libhsmxface.so"/>
    <PARAMETER NAME="FUNCTION" VALUE="HSM_HANDLER"/>
    <PARAMETER NAME="AUTO_TEST" VALUE="ON"/>
    <PARAMETER NAME="DEBUG" VALUE="OFF"/>
  </MODULE>
  <MODULE TYPE="RECEIVETYPE">
    <PARAMETER NAME="RECEIVETHREAD" VALUE="DISABLED"/>
    <PARAMETER NAME="DLL" VALUE="lib/libtcplinks.so"/>
    <PARAMETER NAME="FUNCTION" VALUE="TCP_SINGLE_XF"/>
    <PARAMETER NAME="PROTOCOL" VALUE="TCP_CLIENT"/>
    <PARAMETER NAME="IP_ADDRESS" VALUE="10.13.25.242"/>
    <PARAMETER NAME="IP_PORT" VALUE="1500"/>
    <PARAMETER NAME="CALLS_PERIOD" VALUE="2000" COMMENT="in msec"/>
    <PARAMETER NAME="RCV_TIMEOUT" VALUE="5" COMMENT="in sec"/>
    <PARAMETER NAME="MESS_CODING" VALUE="OFF"/>
    <PARAMETER NAME="HEADER_TYPE" VALUE="BIN_LENGTH_2"/>
    <PARAMETER NAME="DEBUG" VALUE="OFF"/>
  </MODULE>
  <MODULE TYPE="TRANSMITTYPE">
    <PARAMETER NAME="DLL" VALUE="lib/libtcplinks.so"/>
    <PARAMETER NAME="FUNCTION" VALUE="TCP_SINGLE_XF"/>
    <PARAMETER NAME="SEND_TIMEOUT" VALUE="5" COMMENT="in sec"/>
    <PARAMETER NAME="DEBUG" VALUE="OFF"/>
  </MODULE>
  <MODULE TYPE="ROUTING">
    <PARAMETER NAME="CFILE" VALUE="conf/tables/noroute.xml"/>
  </MODULE>
</CHANNEL>
