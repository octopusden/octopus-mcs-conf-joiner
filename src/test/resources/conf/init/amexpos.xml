
<CHANNEL NAME="AMEXPOS">
  <PARAMETER NAME="MESSTRACE" VALUE="OFF"/>
  <PARAMETER NAME="USERINTERFACE" VALUE="CONSOLE"/>
  <PARAMETER NAME="CHANNEL_ZPK" VALUE="0000000000000000"/>
  <PARAMETER NAME="EXTERNAL_IS0" VALUE="amex_pos.prm"/>
  <PARAMETER NAME="LOG_ALL_INT" VALUE="OFF"/>
  <PARAMETER NAME="LOG_ALL_EXT" VALUE="OFF"/>
  <PARAMETER NAME="LOG_CVV_TAG" VALUE="OFF"/>
  <MODULE TYPE="FORMAT">
    <PARAMETER NAME="DLL" VALUE="lib/libengine4m.so"/>
    <PARAMETER NAME="FUNCTION" VALUE="IS08583_FORMAT"/>
    <PARAMETER NAME="CODE" VALUE="EBCDIC"/>
    <PARAMETER NAME="MESS_FORMAT" VALUE="IS08583"/>
    <PARAMETER NAME="DEBUG" VALUE="OFF"/>
  </MODULE>
  <MODULE TYPE="ANALYSE">
    <PARAMETER NAME="DLL" VALUE="lib/libengine4m.so"/>
    <PARAMETER NAME="FUNCTION" VALUE="IS08583_ANALYSE"/>
    <PARAMETER NAME="DEBUG" VALUE="OFF"/>
  </MODULE>
  <MODULE TYPE="NETWORK">
    <PARAMETER NAME="DLL" VALUE="amexiso/libamexiso.so"/>
    <PARAMETER NAME="FUNCTION" VALUE="AMEXIS0_NETWORK"/>
    <PARAMETER NAME="MATCHING" VALUE="ADAPTIVE"/>
    <PARAMETER NAME="SIGNON_CHECK" VALUE="OFF"/>
    <PARAMETER NAME="LINK_CHECK" VALUE="OFF"/>
    <PARAMETER NAME="ASK_ACQUIRER_PARAMS" VALUE="ON"/>
    <PARAMETER NAME="NAME_FOR_AID" VALUE="AMEX_POS_F32"/>
    <PARAMETER NAME="DEBUG" VALUE="OFF"/>
  </MODULE>
  <MODULE TYPE="DBACCESS">
    <PARAMETER NAME="CHANNEL_ID" VALUE="AP"/>
    <!--PARAMETER NAME="SQLINIFILE" VALUE="conf/sql/amexpos.sql"/-->

    <PARAMETER NAME="TABLE1" VALUE="COMS_LOG"/>
    <PARAMETER NAME="DEBUG" VALUE="OFF"/>
    <PARAMETER NAME="INCLUDE" VALUE="conf/init/connect.xml"/>
  </MODULE>
  <MODULE TYPE="INTERNAL">
    <PARAMETER NAME="DLL" VALUE="amexiso/libamexiso.so"/>
    <PARAMETER NAME="FUNCTION" VALUE="AMEXIS0_INTERNAL"/>
    <PARAMETER NAME="SIGNON_INTERVAL_TIMER" VALUE="0" COMMENT="in sec"/>
    <PARAMETER NAME="POLL_INTERVAL_TIMER" VALUE="0" COMMENT="in sec"/>
    <PARAMETER NAME="DEBUG" VALUE="OFF"/>
  </MODULE>
  <MODULE TYPE="INTERNAL_CONSTANTS">
    <PARAMETER NAME="FIELD 3.2" VALUE="00" COMMENT="Source Account"/>
  </MODULE>
  <MODULE TYPE="INTERNAL_DEFAULTS">
    <PARAMETER NAME="FIELD 25" VALUE="0000" COMMENT="Message Reason Code"/>
    <!--PARAMETER NAME="FIELD 32" VALUE="643999" COMMENT="AID"/-->

  </MODULE>
  <MODULE TYPE="RECEIVETYPE">
    <PARAMETER NAME="DLL" VALUE="padtrtcp/libpadtrtcp.so"/>
    <PARAMETER NAME="FUNCTION" VALUE="PAD_TRANSPORT"/>
    <PARAMETER NAME="WAIT_MESSAGE_TIMEOUT" VALUE="5" COMMENT="in sec"/>
    <PARAMETER NAME="RCV_TIMEOUT" VALUE="5" COMMENT="in sec"/>
    <PARAMETER NAME="CONFIRM_TIMEOUT" VALUE="1" COMMENT="in sec"/>
    <PARAMETER NAME="DUMP_RCV_DATA" VALUE="OFF"/>
    <PARAMETER NAME="DEBUG" VALUE="OFF"/>
  </MODULE>
  <MODULE TYPE="TRANSMITTYPE">
    <PARAMETER NAME="DLL" VALUE="padtrtcp/libpadtrtcp.so"/>
    <PARAMETER NAME="FUNCTION" VALUE="PAD_TRANSPORT"/>
    <PARAMETER NAME="COM_PORTS" VALUE="201-208"/>
    <!--PARAMETER NAME="CALLED_X25_ADDRESS" VALUE="26245690098219" COMMENT="Frankfurt Stratus NUA"/-->

    <PARAMETER NAME="CALLED_X25_ADDRESS" VALUE="25017720019093" COMMENT="Moscow NUA"/>
    <PARAMETER NAME="X25_CALL_TIMEOUT" VALUE="5" COMMENT="in sec"/>
    <PARAMETER NAME="SEND_TIMEOUT" VALUE="10" COMMENT="in sec"/>
    <PARAMETER NAME="DUMP_SND_DATA" VALUE="OFF"/>
    <PARAMETER NAME="DEBUG" VALUE="OFF"/>
  </MODULE>
  <MODULE TYPE="ROUTING">
    <PARAMETER NAME="CFILE" VALUE="conf/tables/xsmroutes.xml"/>
  </MODULE>
</CHANNEL>
