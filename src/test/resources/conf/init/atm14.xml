
<CHANNEL NAME="ATM_14_TCP">
  <PARAMETER NAME="USERINTERFACE" VALUE="CONSOLE"/>
  <PARAMETER NAME="EXTERNAL_ISO" VALUE="atm_ctrl.prm"/>
  <PARAMETER NAME="RID_ROUTING" VALUE="ON"/>
  <MODULE TYPE="ANALYSE">
    <PARAMETER NAME="DEBUG" VALUE="OFF"/>
    <PARAMETER NAME="DLL" VALUE="atmctrl/libatmctrl.so"/>
    <PARAMETER NAME="FUNCTION" VALUE="ANALYSE_ATM_MESSAGE"/>
  </MODULE>
  <MODULE TYPE="FORMAT">
    <PARAMETER NAME="DEBUG" VALUE="OFF"/>
    <PARAMETER NAME="DLL" VALUE="atmctrl/libatmctrl.so"/>
    <PARAMETER NAME="FUNCTION" VALUE="FORMAT_ATM_MESSAGE"/>
  </MODULE>
  <MODULE TYPE="NETWORK">
    <PARAMETER NAME="DEBUG" VALUE="OFF"/>
    <PARAMETER NAME="DLL" VALUE="atmctrl/libatmctrl.so"/>
    <PARAMETER NAME="FUNCTION" VALUE="ATM_NETWORK"/>
    <PARAMETER NAME="DRIVER" VALUE="MDS912,NDC+"/>
    <PARAMETER NAME="TCP_ATM_NETWORK" VALUE="YES"/>
    <PARAMETER NAME="MALFUNCTION_ERROR" VALUE="196"/>
    <PARAMETER NAME="RESOURCE_LIMIT" VALUE="64"/>
    <PARAMETER NAME="ATM_TIMER_03" VALUE="68" COMMENT="in sec"/>
    <PARAMETER NAME="CMD_TIMER" VALUE="60" COMMENT="in sec"/>
    <PARAMETER NAME="MAXIMUM_NOTES" VALUE="60"/>
    <PARAMETER NAME="REPEAT_COUNTER" VALUE="5"/>
    <PARAMETER NAME="MAX_START_PAN_PRN" VALUE="10"/>
    <PARAMETER NAME="MAX_END_PAN_PRN" VALUE="30"/>
    <PARAMETER NAME="RC_TABLE" VALUE="conf/atm/rc.xml"/>
    <PARAMETER NAME="FORMATFILE" VALUE="atmctrl/formats.xml"/>
    <PARAMETER NAME="MAP_TABLE" VALUE="conf/atm/encoding.xml"/>
    <PARAMETER NAME="DATA_MAPPING_TABLE" VALUE="atmctrl/data_map.xml"/>
    <PARAMETER NAME="OPERATION_KEY_FILE" VALUE="conf/atm/key.xml"/>
    <PARAMETER NAME="CONSUMER_RECEIPT" VALUE="conf/atm/c_receip.def"/>
    <PARAMETER NAME="JOURNAL_RECEIPT" VALUE="conf/atm/j_receip.def"/>
    <PARAMETER NAME="STATEMENT_RECEIPT" VALUE="conf/atm/s_receip.def"/>
    <PARAMETER NAME="ADMIN_RECEIPT" VALUE="conf/atm/a_receip.def"/>
    <PARAMETER NAME="SCREENS_TEMPLATE" VALUE="conf/atm/scr_tmpl.def"/>
    <PARAMETER NAME="CONVERT_TPK_ZPK" VALUE="OFF"/>
  </MODULE>
  <MODULE TYPE="INTERNAL">
    <PARAMETER NAME="DEBUG" VALUE="OFF"/>
    <PARAMETER NAME="DLL" VALUE="atmctrl/libatmctrl.so"/>
    <PARAMETER NAME="FUNCTION" VALUE="ATM_INTERNAL"/>
    <!--PARAMETER NAME="TRACE_FILE" VALUE="atmlog14"/-->

    <PARAMETER NAME="TRACE_SIZE" VALUE="1" COMMENT="in MByte"/>
    <PARAMETER NAME="BACK_NUM" VALUE="10"/>
  </MODULE>
  <MODULE TYPE="DBACCESS">
    <PARAMETER NAME="CHANNEL_ID" VALUE="A1"/>
    <!--PARAMETER NAME="SQLINIFILE" VALUE="conf/sql/atm14.sql"/-->

    <PARAMETER NAME="DEBUG" VALUE="OFF"/>
    <PARAMETER NAME="TABLE1" VALUE="COMS_LOG"/>
    <PARAMETER NAME="INCLUDE" VALUE="conf/init/connect.xml"/>
    <PARAMETER NAME="MAX_SESSIONS" VALUE="7"/>
  </MODULE>
  <MODULE TYPE="RECEIVETYPE">
    <PARAMETER NAME="DEBUG" VALUE="OFF"/>
    <PARAMETER NAME="DLL" VALUE="lib/libtcplinks.so"/>
    <PARAMETER NAME="FUNCTION" VALUE="TCP_MULTI_XF"/>
    <PARAMETER NAME="PROTOCOL" VALUE="TCP_SERVER"/>
    <PARAMETER NAME="IP_PORT" VALUE="10520"/>
    <PARAMETER NAME="ATM_SOCKETS" VALUE="ON"/>
    <PARAMETER NAME="HEADER_TYPE" VALUE="BIN_LENGTH_2"/>
    <PARAMETER NAME="STATUS_REPORT" VALUE="ON"/>
    <PARAMETER NAME="CHECK_RECALLS" VALUE="ON"/>
    <PARAMETER NAME="USE_CONNECT_ID" VALUE="OFF"/>
    <PARAMETER NAME="KEEP_LISTENER" VALUE="ON"/>
    <PARAMETER NAME="KEEP_ALIVE" VALUE="ON"/>
    <!--PARAMETER NAME="LISTENING_TIMEOUT" VALUE="60" COMMENT="in sec"/-->
    <!--PARAMETER NAME="LISTENER_BACKLOG" VALUE="3"/-->

  </MODULE>
  <MODULE TYPE="TRANSMITTYPE">
    <PARAMETER NAME="DEBUG" VALUE="OFF"/>
    <PARAMETER NAME="ATM_NETWORK" VALUE="ON"/>
    <PARAMETER NAME="DLL" VALUE="lib/libtcplinks.so"/>
    <PARAMETER NAME="FUNCTION" VALUE="TCP_MULTI_XF"/>
  </MODULE>
  <MODULE TYPE="CURRENCY_TABLE">
    <PARAMETER NAME="CFILE" VALUE="conf/tables/currency.xml"/>
  </MODULE>
  <MODULE TYPE="ROUTING">
    <PARAMETER NAME="CFILE" VALUE="conf/tables/routing.xml"/>
  </MODULE>
</CHANNEL>
