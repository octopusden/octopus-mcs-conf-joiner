
<CHANNEL NAME="CB_GATE">
  <PARAMETER NAME="MESSTRACE" VALUE="OFF"/>
  <PARAMETER NAME="USERINTERFACE" VALUE="CONSOLE"/>
  <MODULE TYPE="INTERNAL">
    <PARAMETER NAME="DLL" VALUE="standin/libstandin.so"/>
    <PARAMETER NAME="FUNCTION" VALUE="STANDIN_INTERNAL"/>
    <PARAMETER NAME="TRANSACTION_TIMEOUT" VALUE="35" COMMENT="in sec"/>
    <PARAMETER NAME="TOKEN_TYPE_CODE" VALUE="CARD"/>
    <PARAMETER NAME="MESSAGE_TYPES" VALUE="conf/tables/gate_msg_fncl.xml"/>
    <PARAMETER NAME="STAND-IN_AUTHORIZATION" VALUE="OFF"/>
    <PARAMETER NAME="DEBUG" VALUE="OFF"/>
  </MODULE>
  <MODULE TYPE="DBACCESS">
    <PARAMETER NAME="CHANNEL_ID" VALUE="T1"/>
    <!--PARAMETER NAME="SQLINIFILE" VALUE="conf/sql/cb_gate.sql"/-->

    <PARAMETER NAME="MAX_SESSIONS" VALUE="2"/>
    <PARAMETER NAME="INCLUDE" VALUE="conf/init/connect.xml"/>
    <PARAMETER NAME="DEBUG" VALUE="OFF"/>
  </MODULE>
  <MODULE TYPE="RESPONSE_CODES">
    <PARAMETER NAME="CFILE" VALUE="conf/tables/rcodes.xml"/>
  </MODULE>
  <MODULE TYPE="ROUTING">
    <PARAMETER NAME="CFILE" VALUE="conf/tables/sysroute_cbg.xml"/>
  </MODULE>
</CHANNEL>
