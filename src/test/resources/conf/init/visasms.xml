
<CHANNEL NAME="VISASMS">
  <PARAMETER NAME="MESSTRACE" VALUE="OFF"/>
  <PARAMETER NAME="USERINTERFACE" VALUE="CONSOLE"/>
  <PARAMETER NAME="EXTERNAL_IS0" VALUE="visa_sms.prm" COMMENT="CCD Mode"/>
  <!--PARAMETER NAME="LOG_ALL_INT" VALUE="ON"/>
  <PARAMETER NAME="LOG_ALL_EXT" VALUE="ON"/>
  <PARAMETER NAME="LOG_CVV_TAG" VALUE="ON"/-->

  <MODULE TYPE="FORMAT">
    <PARAMETER NAME="DLL" VALUE="lib/libengine4m.so"/>
    <PARAMETER NAME="FUNCTION" VALUE="IS08583_FORMAT"/>
    <PARAMETER NAME="MESS_FORMAT" VALUE="VISA"/>
    <PARAMETER NAME="CONVERT_REJECT_TO_RESPONSE" VALUE="ON"/>
    <PARAMETER NAME="DEBUG" VALUE="OFF"/>
  </MODULE>
  <MODULE TYPE="ANALYSE">
    <PARAMETER NAME="DLL" VALUE="lib/libengine4m.so"/>
    <PARAMETER NAME="FUNCTION" VALUE="IS08583_ANALYSE"/>
    <PARAMETER NAME="DEBUG" VALUE="OFF"/>
  </MODULE>
  <MODULE TYPE="NETWORK">
    <PARAMETER NAME="DLL" VALUE="visanet/libvisanet.so"/>
    <PARAMETER NAME="FUNCTION" VALUE="VISA_NETWORK"/>
    <PARAMETER NAME="MATCHING" VALUE="ADAPTIVE"/>
    <PARAMETER NAME="ASK_ACQUIRER_PARAMS" VALUE="ON" COMMENT="ATM-ROUTER"/>
    <PARAMETER NAME="CHANGE_INCOMING_CRYPTOGRAM_CODING" VALUE="OFF" COMMENT="CCD Mode"/>
    <PARAMETER NAME="LINK_CHECK" VALUE="ON"/>
    <PARAMETER NAME="SIGNON_CHECK" VALUE="OFF"/>
    <PARAMETER NAME="DEBUG" VALUE="OFF"/>
  </MODULE>
  <MODULE TYPE="DBACCESS">
    <PARAMETER NAME="CHANNEL_ID" VALUE="S1"/>
    <!--PARAMETER NAME="SQLINIFILE" VALUE="conf/sql/visasms.sql"/-->

    <PARAMETER NAME="TABLE1" VALUE="COMS_LOG"/>
    <PARAMETER NAME="INCLUDE" VALUE="conf/init/connect.xml"/>
    <PARAMETER NAME="SAF_CHANNEL_ID" VALUE="SF"/>
    <PARAMETER NAME="DEBUG" VALUE="OFF"/>
  </MODULE>
  <MODULE TYPE="INTERNAL">
    <PARAMETER NAME="DLL" VALUE="visanet/libvisanet.so"/>
    <PARAMETER NAME="FUNCTION" VALUE="VISA_INTERNAL"/>
    <PARAMETER NAME="STATION_ADDRESS" VALUE="974321"/>
    <PARAMETER NAME="MESSAGE_FORMAT" VALUE="VIP"/>
    <PARAMETER NAME="ATM_SMS_FLAG" VALUE="1"/>
    <PARAMETER NAME="PRIORITY_ROUTING" VALUE="1"/>
    <PARAMETER NAME="F07_CORRECTION" VALUE="1"/>
    <PARAMETER NAME="AUTOSIGNON" VALUE="5" COMMENT="in sec"/>
    <PARAMETER NAME="NET_TEST_PERIOD" VALUE="10" COMMENT="in min"/>
    <PARAMETER NAME="CURRENCY_810_TO_643" VALUE="1" COMMENT="Do Not Set Before 1 Oct 2004"/>
    <PARAMETER NAME="CHIP_DATA_TRANSPORT" VALUE="FIELD_55_CCD" COMMENT="CCD Mode"/>
    <PARAMETER NAME="CHANGE_OUTGOING_CRYPTOGRAM_CODING" VALUE="0" COMMENT="CCD Mode"/>
    <PARAMETER NAME="AUTORESPONSE" VALUE="1"/>
    <PARAMETER NAME="AUTORESPONSE_ON_REVERSALS" VALUE="0"/>
    <PARAMETER NAME="AUTORESPONSE_ON_ADVICES" VALUE="0"/>
    <PARAMETER NAME="SAF_MESSAGE_TYPES" VALUE="0420" COMMENT="Limited set of advice messages"/>
    <PARAMETER NAME="SAF_TO_NET_SEND_PERIOD" VALUE="5" COMMENT="in sec"/>
    <!--PARAMETER NAME="MAG_STRIPE_SERVICE_CODES_TO_BLOCK" VALUE="6*,2*"/>
    <PARAMETER NAME="MAG_STRIPE_SERVICE_CODES_TO_BLOCK_ATM" VALUE="6*,2*"/>
    <PARAMETER NAME="MAG_STRIPE_SERVICE_CODES_TO_BLOCK_POS" VALUE="6*,2*"/-->

    <PARAMETER NAME="DEBUG" VALUE="OFF"/>
  </MODULE>
  <MODULE TYPE="INTERNAL_CONSTANTS">
    <PARAMETER NAME="FIELD 19" VALUE="643" COMMENT="ACQ COUNTRY CODE"/>
    <PARAMETER NAME="FIELD 53" VALUE="2001010100000000" COMMENT="SECURITY INFORMATION"/>
    <PARAMETER NAME="FIELD 66" VALUE="9" COMMENT="SETTLEMENT CODE"/>
  </MODULE>
  <MODULE TYPE="INTERNAL_DEFAULTS">
    <PARAMETER NAME="FIELD 25" VALUE="00" COMMENT="POS CONDITION CODE"/>
    <PARAMETER NAME="FIELD 33" VALUE="404111" COMMENT="FORWARDING INSTITUTION ID"/>
    <PARAMETER NAME="FIELD 26" VALUE="12" COMMENT="POS PIN CAPTURE CODE"/>
    <PARAMETER NAME="FIELD 60.1" VALUE="0" COMMENT="TERMINAL TYPE"/>
    <PARAMETER NAME="FIELD 60.2" VALUE="0" COMMENT="TERMINAL ENTRY CAPABILITY"/>
    <PARAMETER NAME="FIELD 62.1" VALUE="Y" COMMENT="AUTH. CHARACTERIST. INDICATOR"/>
    <PARAMETER NAME="FIELD 63.1" VALUE="0002" COMMENT="NETWORK ID (VISANET)"/>
    <PARAMETER NAME="FIELD 63.3" VALUE="2501" COMMENT="MESSAGE REASON CODE"/>
    <PARAMETER NAME="FIELD 63.5" VALUE="000217" COMMENT="PMC ID"/>
    <PARAMETER NAME="FIELD 63.11" VALUE="B" COMMENT="ATTRIBUTE CODE"/>
    <PARAMETER NAME="FIELD 99" VALUE="404111" COMMENT="SETTLEMENT INSTITUTION ID"/>
    <PARAMETER NAME="FIELD 100" VALUE="404111" COMMENT="RECEIVING INSTITUTION ID"/>
  </MODULE>
  <MODULE TYPE="RECEIVETYPE">
    <PARAMETER NAME="DLL" VALUE="lib/libtcplinks.so"/>
    <PARAMETER NAME="FUNCTION" VALUE="TCP_SINGLE_XF"/>
    <PARAMETER NAME="PROTOCOL" VALUE="TCP_CLIENT"/>
    <!--PARAMETER NAME="IP_ADDRESS" VALUE="10.228.0.35"/-->
    <!--PARAMETER NAME="IP_PORT" VALUE="55286"/-->

    <PARAMETER NAME="IP_ADDRESS" VALUE="209.65.33.52"/>
    <PARAMETER NAME="IP_PORT" VALUE="10101"/>
    <PARAMETER NAME="KEEP_ALIVE" VALUE="ON"/>
    <PARAMETER NAME="CALLS_PERIOD" VALUE="2000" COMMENT="in msec"/>
    <PARAMETER NAME="HEADER_TYPE" VALUE="VISA_STYLE"/>
    <PARAMETER NAME="STATUS_REPORT" VALUE="ON"/>
    <PARAMETER NAME="DUMP_RCV_DATA" VALUE="OFF"/>
    <PARAMETER NAME="DEBUG" VALUE="OFF"/>
  </MODULE>
  <MODULE TYPE="TRANSMITTYPE">
    <PARAMETER NAME="DLL" VALUE="lib/libtcplinks.so"/>
    <PARAMETER NAME="FUNCTION" VALUE="TCP_SINGLE_XF"/>
    <PARAMETER NAME="SEND_TIMEOUT" VALUE="5" COMMENT="in sec"/>
    <PARAMETER NAME="DUMP_SND_DATA" VALUE="OFF"/>
    <PARAMETER NAME="DEBUG" VALUE="OFF"/>
  </MODULE>
  <MODULE TYPE="ROUTING">
    <PARAMETER NAME="CFILE" VALUE="conf/tables/routing.xml"/>
  </MODULE>
</CHANNEL>
