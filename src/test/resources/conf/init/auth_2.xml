
<CHANNEL NAME="OWN_AUTH_2">
  <PARAMETER NAME="MESSTRACE" VALUE="OFF"/>
  <PARAMETER NAME="USERINTERFACE" VALUE="CONSOLE"/>
  <PARAMETER NAME="LOG_ALL_INT" VALUE="OFF"/>
  <PARAMETER NAME="LOG_ALL_EXT" VALUE="OFF"/>
  <PARAMETER NAME="LOG_CVV_TAG" VALUE="OFF"/>
  <MODULE TYPE="INTERNAL">
    <PARAMETER NAME="TEST_DEF" VALUE="SYS"/>
    <PARAMETER NAME="DEBUG" VALUE="OFF"/>
    <PARAMETER NAME="DLL" VALUE="auth/libauth.so"/>
    <PARAMETER NAME="FUNCTION" VALUE="AUTHORIZATION"/>
    <PARAMETER NAME="IS_AUTH_BALANCE" VALUE="YES"/>
    <PARAMETER NAME="MIN_STATISTICS_INTERVAL" VALUE="60000" COMMENT="in msec"/>
    <PARAMETER NAME="STATISTICS_THRESHOLD" VALUE="100" COMMENT="in msec"/>
    <PARAMETER NAME="SET_GL_CONST_INTERVAL" VALUE="3600000" COMMENT="in msec"/>
    <PARAMETER NAME="LOG_MODE" VALUE="BRIEF"/>
    <PARAMETER NAME="VERIFICATION_DAC" VALUE="0"/>
    <PARAMETER NAME="VERIFICATION_DN" VALUE="0"/>
    <PARAMETER NAME="INFO_SERVICE" VALUE="#A0211:BILLING1;"/>
    <PARAMETER NAME="PARTIAL_GRADE_ALLOWED" VALUE="1"/>
  </MODULE>
  <MODULE TYPE="DBACCESS">
    <PARAMETER NAME="CHANNEL_ID" VALUE="AU"/>
    <!--PARAMETER NAME="SQLINIFILE" VALUE="conf/sql/auth.sql"/-->

    <PARAMETER NAME="DEBUG" VALUE="OFF"/>
    <PARAMETER NAME="INCLUDE" VALUE="conf/init/connect.xml"/>
    <PARAMETER NAME="MAX_SESSIONS" VALUE="10"/>
  </MODULE>
  <MODULE TYPE="RESPONSE_CODES">
    <PARAMETER NAME="CFILE" VALUE="conf/tables/rcodes.xml"/>
  </MODULE>
  <MODULE TYPE="ROUTING">
    <PARAMETER NAME="CFILE" VALUE="conf/tables/authroutes.xml"/>
  </MODULE>
</CHANNEL>
