
<CHANNEL NAME="INTRA_NET">
  <PARAMETER NAME="MESSTRACE" VALUE="OFF"/>
  <PARAMETER NAME="USERINTERFACE" VALUE="CONSOLE"/>
  <PARAMETER NAME="RID_ROUTING" VALUE="ON"/>
  <MODULE TYPE="DBACCESS">
    <PARAMETER NAME="CHANNEL_ID" VALUE="I0"/>
    <!--PARAMETER NAME="SQLINIFILE" VALUE="conf/sql/intranet.sql"/-->

    <PARAMETER NAME="DEBUG" VALUE="OFF"/>
    <PARAMETER NAME="INCLUDE" VALUE="conf/init/connect.xml"/>
  </MODULE>
  <MODULE TYPE="NETWORK">
    <PARAMETER NAME="DLL" VALUE="intranet/libintranet.so"/>
    <PARAMETER NAME="FUNCTION" VALUE="INTRA_NETWORK"/>
    <PARAMETER NAME="MAILBOX_TYPE" VALUE="ODBC"/>
    <PARAMETER NAME="INTRANET_ID" VALUE="899"/>
    <PARAMETER NAME="POLLING_PERIOD" VALUE="1000" COMMENT="in msec"/>
    <PARAMETER NAME="DEBUG" VALUE="OFF"/>
  </MODULE>
  <MODULE TYPE="INTERNAL">
    <PARAMETER NAME="DLL" VALUE="intranet/libintranet.so"/>
    <PARAMETER NAME="FUNCTION" VALUE="INTRA_INTERNAL"/>
    <PARAMETER NAME="SERVER_2" VALUE="865" COMMENT="wow4u_ir_bal first instance"/>
    <!--PARAMETER NAME="SERVER_2" VALUE="866" COMMENT="backup wow4u_ir_bal second instance"/-->
    <PARAMETER NAME="SERVER_3" VALUE="858" COMMENT="wow4u city+ first instance"/>
    <!--PARAMETER NAME="SERVER_3" VALUE="860" COMMENT="backup wow4u city+ second instance"/-->
    <PARAMETER NAME="SERVER_4" VALUE="863" COMMENT="wow4u cash-in first instance"/>
    <!--PARAMETER NAME="SERVER_4" VALUE="864" COMMENT="backup wow4u cash-in second instance"/-->
    <PARAMETER NAME="DEBUG" VALUE="OFF"/>
  </MODULE>
  <MODULE TYPE="ROUTING">
    <PARAMETER NAME="CFILE" VALUE="conf/tables/routing.xml"/>
  </MODULE>
</CHANNEL>
