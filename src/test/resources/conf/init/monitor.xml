
<CHANNEL NAME="MONITOR">
  <PARAMETER NAME="USERINTERFACE" VALUE="CONSOLE"/>
  <MODULE TYPE="INTERNAL">
    <PARAMETER NAME="DLL" VALUE="monitor/libmonitor.so"/>
    <PARAMETER NAME="FUNCTION" VALUE="MONITOR_INTERNAL"/>
  </MODULE>
  <MODULE TYPE="RECEIVETYPE">
    <PARAMETER NAME="DLL" VALUE="monitor/libmonitor.so"/>
    <PARAMETER NAME="FUNCTION" VALUE="MONITOR_PIPESERVER"/>
    <PARAMETER NAME="LOGFILE" VALUE="message.log"/>
    <PARAMETER NAME="LOGFILECREATION" VALUE="ON"/>
    <PARAMETER NAME="PIPENAME" VALUE="pipe/logpipe"/>
    <PARAMETER NAME="PRIORITY" VALUE="4"/>
    <PARAMETER NAME="DEBUG" VALUE="OFF"/>
    <PARAMETER NAME="LOG_SIZE" VALUE="10" COMMENT="in MByte"/>
    <PARAMETER NAME="BACK_NUM" VALUE="999"/>
    <PARAMETER NAME="BACK_CHECK" VALUE="1"/>
  </MODULE>
  <MODULE TYPE="ROUTING">
    <PARAMETER NAME="CFILE" VALUE="conf/tables/noroute.xml"/>
  </MODULE>
</CHANNEL>
