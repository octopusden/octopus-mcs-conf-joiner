<?xml version="1.0" encoding="UTF-8"?>
<CHANNEL>
  <USER_PARAMETER NAME="MESSTRACE"/>
  <USER_PARAMETER NAME="LOG_ALL_INT" VALUE="OFF"/>
  <USER_PARAMETER NAME="LOG_ALL_EXT" VALUE="OFF"/>
  <USER_PARAMETER NAME="LOG_CVV_TAG" VALUE="OFF"/>
  <USER_PARAMETER NAME="CHANNEL_ZPK"/>
  <USER_PARAMETER NAME="LOG_THREAD_ID"/>
  <USER_PARAMETER NAME="CHANNEL_AWK"/>
  <USER_PARAMETER NAME="CHANNEL_IWK"/>

  <!-- Own Authorization Modules -->
  <MODULE NAME="AUTH_AFFILIATED" TYPE="INTERNAL" DLL="AUTH" FUNCTION="AUTHORIZATION_AFF">
    <USER_PARAMETER NAME="LOG_MODE" VALUE="NONE"/>
    <!-- Include common Internal -->
    <INCLUDE MODULE_NAME="COMMON_INTERNAL"/>
  </MODULE>


  <MODULE NAME="AUTH_INTERNAL" TYPE="INTERNAL" DLL="AUTH">
    <USER_PARAMETER NAME="LOG_MODE" VALUE="NONE"/>
    <!-- Include common Internal -->
    <INCLUDE MODULE_NAME="COMMON_INTERNAL"/>
  </MODULE>


  <MODULE NAME="AFFILIATED_CHANNEL" TYPE="INTERNAL" DLL="CUSTOMS" FUNCTION="CUSTOMS_INTERNAL">
    <USER_PARAMETER NAME="COMS_LOG_MODE" VALUE="FULL"/>
    <!-- Include common Internal -->
    <INCLUDE MODULE_NAME="COMMON_INTERNAL"/>
  </MODULE>


  <!-- POS Controller 2 Modules -->
  <MODULE NAME="POSCTRL2_NETWORK" TYPE="NETWORK" DLL="POSCTRL2" FUNCTION="POS_NETWORK">
    <!-- No default value, leave only name in init folder!!!! -->
    <USER_PARAMETER NAME="ADDITIONAL_PARAMETERS"/>
    <!-- Obsolete -->
    <OBSOLETE_PARAMETER NAME="ACQUIRER_FEE"/>
    <OBSOLETE_PARAMETER NAME="CREDIT_OFFLINE_CARD_VERIFICATION_CHANNELS"/>
    <OBSOLETE_PARAMETER NAME="FIELD_22_CORRECTION"/>
    <OBSOLETE_PARAMETER NAME="NEW_CREDIT_MESSAGE"/>
    <OBSOLETE_PARAMETER NAME="PROC_CODE_CHECK"/>
    <OBSOLETE_PARAMETER NAME="PROCESS_CUSTOM_DATA_FROM_DB"/>
    <OBSOLETE_PARAMETER NAME="CORRECTORRESPONSE"/>
    <OBSOLETE_PARAMETER NAME="CARD_VERIFICATION_CHANNELS"/>
    <!-- Include common NetWork Module -->
    <INCLUDE MODULE_NAME="COMMON_NETWORK"/>
  </MODULE>

  <!-- POS Controller 1 Modules (Obsolete) -->
  <MODULE NAME="POSCTRL1_NETWORK" TYPE="NETWORK" DLL="POSCTRL" FUNCTION="POS_NETWORK">
    <!-- No default value, leave only name in init folder!!!! -->
    <USER_PARAMETER NAME="ADDITIONAL_PARAMETERS"/>
    <USER_PARAMETER NAME="CARD_VERIFICATION_CHANNELS"/>
    <!-- Obsolete -->
    <OBSOLETE_PARAMETER NAME="CORRECTORRESPONSE"/>
    <!-- Include common NetWork Module -->
    <INCLUDE MODULE_NAME="COMMON_NETWORK"/>
  </MODULE>

  <!-- ATM Controller Modules -->
  <MODULE NAME="ATM_NETWORK" TYPE="NETWORK" DLL="ATMCTRL" FUNCTION="ATM_NETWORK">
    <USER_PARAMETER NAME="MALFUNCTION_ERROR" VALUE="196"/>
    <USER_PARAMETER NAME="STATUS_STRING" VALUE="0,1,2,2,4,5"/>
    <USER_PARAMETER NAME="ATM_TIMER_03" VALUE="90" COMMENT="in sec"/>
    <USER_PARAMETER NAME="CMD_TIMER" VALUE="30" COMMENT="in sec"/>
    <USER_PARAMETER NAME="REPEAT_COUNTER" VALUE="3"/>
    <USER_PARAMETER NAME="MAXIMUM_NOTES" VALUE="50"/>
    <USER_PARAMETER NAME="MAXIMUM_COINS" VALUE="25"/>
    <USER_PARAMETER NAME="ALT_REV_RC" VALUE="OFF"/>
    <USER_PARAMETER NAME="MAX_START_PAN_PRN" VALUE="10"/>
    <USER_PARAMETER NAME="MAX_END_PAN_PRN" VALUE="30"/>
    <USER_PARAMETER NAME="SAF_TO_INT_SEND_PERIOD" VALUE="1" COMMENT="in sec"/>
    <USER_PARAMETER NAME="SAF_TRACE_LEVEL" VALUE="OFF"/>
    <!-- Include common NetWork Module -->
    <INCLUDE MODULE_NAME="COMMON_NETWORK"/>
  </MODULE>

  <MODULE NAME="ATM_INTERNAL" TYPE="INTERNAL" DLL="ATMCTRL" FUNCTION="ATM_INTERNAL">
    <USER_PARAMETER NAME="TRACE_FILE"/>
    <USER_PARAMETER NAME="TRACE_SIZE" VALUE="1" COMMENT="in MByte"/>
    <USER_PARAMETER NAME="BACK_NUM" VALUE="10"/>
    <USER_PARAMETER NAME="EJ_DATA_FILE"/>
    <USER_PARAMETER NAME="EJ_DATA_SIZE" VALUE="1" COMMENT="in MByte"/>
    <USER_PARAMETER NAME="EJ_DATA_BACK_NUM" VALUE="10"/>
    <!-- Include common Internal -->
    <INCLUDE MODULE_NAME="COMMON_INTERNAL"/>
  </MODULE>


  <!-- VISA Net -->
  <MODULE NAME="VISANET_INTERNAL" TYPE="INTERNAL" DLL="VISANET" FUNCTION="VISA_INTERNAL">
    <USER_PARAMETER NAME="STATION_ADDRESS"/>
    <!-- Include common Internal -->
    <INCLUDE MODULE_NAME="COMMON_INTERNAL"/>
  </MODULE>

  <USER_MODULE NAME="VISANET_INTERNAL_DEFAULTS" TYPE="INTERNAL_DEFAULTS" DEPEND_ON_MODULE="VISANET_INTERNAL">
    <!-- Those parameters we leave in init folder 
         All other parameters move to user folder!!!
    -->
    <PARAMETER NAME="FIELD 1.1" VALUE="YES" COMMENT="SWITCH TO NEW DATA FIELDS VISA BE 07.2"/>
    <PARAMETER NAME="FIELD 60.1" COMMENT="TERMINAL TYPE"/>
    <PARAMETER NAME="FIELD 60.2" COMMENT="TERMINAL ENTRY CAPABILITY"/>
    <PARAMETER NAME="FIELD 62.1" COMMENT="AUTH. CHARACTERIST. INDICATOR"/>
  </USER_MODULE>

  <USER_MODULE NAME="VISANET_ISSUER_KEYS" TYPE="ISSUER_KEYS" DLL="VISANET"/>

  <!-- Datagate -->
  <MODULE NAME="DATAGATE_NETWORK" TYPE="NETWORK" DLL="DATAGATE">
    <USER_PARAMETER NAME="FILE_BASE_DIR"/>
    <USER_PARAMETER NAME="SOAPLOGDIR"/>
    <!-- Include common NetWork Module -->
    <INCLUDE MODULE_NAME="COMMON_NETWORK"/>
  </MODULE>

  <!-- Intranet -->
  <MODULE NAME="INTRANET_INTERNAL" TYPE="INTERNAL" DLL="INTRANET">
    <USER_PARAMETER NAME="SERVER_1"/>
    <USER_PARAMETER NAME="SERVER_2"/>
    <USER_PARAMETER NAME="SERVER_3"/>
    <USER_PARAMETER NAME="SERVER_4"/>
    <!-- Include common Internal -->
    <INCLUDE MODULE_NAME="COMMON_INTERNAL"/>
  </MODULE>


  <!-- ******************  Transport Modules *********************** -->

  <MODULE NAME="TCPLINKS_RECEIVETYPE" TYPE="RECEIVETYPE" DLL="TCPLINKS">
    <USER_PARAMETER NAME="IP_PORT"/>
    <USER_PARAMETER NAME="HEADER_TYPE"/>
    <USER_PARAMETER NAME="HOSTNAME"/>
    <USER_PARAMETER NAME="IP_ADDRESS"/>
    <USER_PARAMETER NAME="ALT_HOSTNAME"/>
    <USER_PARAMETER NAME="CLIENT_ADDRESS"/>
    <USER_PARAMETER NAME="IP_PORT_TO_CALL"/>
    <USER_PARAMETER NAME="IP_PORT_TO_LISTEN"/>
    <USER_PARAMETER NAME="ALT_IP_PORT"/>
    <USER_PARAMETER NAME="ALT_IP_PORT_TO_CALL"/>
    <USER_PARAMETER NAME="ALT_IP_PORT_TO_LISTEN"/>
    <USER_PARAMETER NAME="VAP_TRANS_KEY"/>
    <USER_PARAMETER NAME="LISTENER_ADDRESS"/>
    <!-- Include common recieve type -->
    <INCLUDE MODULE_NAME="COMMON_RECEIVETYPE"/>
  </MODULE>


  <!-- ********************* Common Modules ************************* -->

  <MODULE NAME="COMMON_INTERNAL" TYPE="INTERNAL">
    <USER_PARAMETER NAME="SAF_TO_NET_SEND_PERIOD"/>
    <USER_PARAMETER NAME="SAF_TO_NET_MAX_THREADS"/>
    <USER_PARAMETER NAME="FIRSTSIGNON"/>
    <USER_PARAMETER NAME="AUTOSIGNON"/>
    <USER_PARAMETER NAME="NET_TEST_PERIOD"/>
    <USER_PARAMETER NAME="DEBUG" VALUE="OFF"/>
    <!-- Obsolete -->
    <OBSOLETE_PARAMETER NAME="HOURS_FROM_GMT"/>
  </MODULE>


  <MODULE NAME="COMMON_NETWORK" TYPE="NETWORK">
    <USER_PARAMETER NAME="DERIV_KEY_A"/>
    <USER_PARAMETER NAME="DERIV_KEY_B"/>
    <USER_PARAMETER NAME="KEY_VARIANT"/>
    <USER_PARAMETER NAME="GUARD_TIME"/>
    <USER_PARAMETER NAME="CHANNEL_HMK"/>
    <USER_PARAMETER NAME="HMK_VARIANT"/>
    <USER_PARAMETER NAME="CHANNEL_ACQ_HMK"/>
    <USER_PARAMETER NAME="ACQ_HMK_VARIANT"/>
    <USER_PARAMETER NAME="CHANNEL_ISS_HMK"/>
    <USER_PARAMETER NAME="ISS_HMK_VARIANT"/>
    <USER_PARAMETER NAME="MESS_AUTH_KEY"/>
    <USER_PARAMETER NAME="DEBUG" VALUE="OFF"/>
  </MODULE>

  <MODULE NAME="COMMON_FORMAT" TYPE="FORMAT">
    <USER_PARAMETER NAME="DEBUG" VALUE="OFF"/>
  </MODULE>

  <MODULE NAME="COMMON_ANALYSE" TYPE="ANALYSE">
    <USER_PARAMETER NAME="DEBUG" VALUE="OFF"/>
  </MODULE>

  <MODULE NAME="COMMON_DBACCESS" TYPE="DBACCESS">
    <USER_PARAMETER NAME="DEBUG" VALUE="OFF"/>
  </MODULE>

  <MODULE NAME="COMMON_RECEIVETYPE" TYPE="RECEIVETYPE">
    <!-- HDLC Specific -->
    <USER_PARAMETER NAME="COM_PORTS"/>
    <USER_PARAMETER NAME="X28_TERMINATOR"/>
    <USER_PARAMETER NAME="X28_CALL_STRING"/>
    <USER_PARAMETER NAME="X28_LISTEN_STRING"/>
    <USER_PARAMETER NAME="LOCAL_X3_PARAMETERS"/>
    <USER_PARAMETER NAME="CALLED_X25_ADDRESS"/>
    <USER_PARAMETER NAME="CALLING_X25_ADDRESS"/>
    <USER_PARAMETER NAME="X25_CALL_FACILITIES"/>
    <USER_PARAMETER NAME="X25_CALL_USER_DATA"/>
    <USER_PARAMETER NAME="REMOTE_X3_PARAMETERS"/>
    <USER_PARAMETER NAME="X28_RESPONSE_TIMEOUT"/>
    <USER_PARAMETER NAME="X25_CALL_TIMEOUT"/>
    <USER_PARAMETER NAME="BAUDRATE"/>
    <USER_PARAMETER NAME="PARITY"/>
    <USER_PARAMETER NAME="DATABITS"/>
    <USER_PARAMETER NAME="STOPBITS"/>
    <USER_PARAMETER NAME="USE_MODEM"/>
    <USER_PARAMETER NAME="MODEM_INIT_STRING"/>
    <USER_PARAMETER NAME="GET_FLAG_TIME"/>
    <USER_PARAMETER NAME="WAKE_UP_TIME"/>
    <USER_PARAMETER NAME="REPLY_TIME"/>
    <USER_PARAMETER NAME="VISA_TIMEOUT_T1"/>
    <USER_PARAMETER NAME="VISA_TIMEOUT_T2"/>
    <USER_PARAMETER NAME="ENQ_FAIL_LIMIT"/>
    <USER_PARAMETER NAME="HDLC_TIMEOUT_T1"/>
    <USER_PARAMETER NAME="HDLC_RETRIES_N2"/>
    <USER_PARAMETER NAME="HDLC_POLL_TIME"/>
    <USER_PARAMETER NAME="HDLC_MAX_FRAME_SIZE"/>
    <!-- Common -->
    <USER_PARAMETER NAME="DEBUG" VALUE="OFF"/>
  </MODULE>

  <MODULE NAME="COMMON_TRANSMITTYPE" TYPE="TRANSMITTYPE">
    <USER_PARAMETER NAME="DEBUG" VALUE="OFF"/>
  </MODULE>


</CHANNEL>



